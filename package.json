{
  "name": "react-roads",
  "version": "1.0.0",
  "description": "A React router designed to allow developers to easily create transitions/animations between pages",
  "main": "dist/main.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode development --env.development",
    "build": "webpack --config webpack.config.js --mode production --env.production && yarn run types",
    "deploy": "yarn run types && yarn run build && yarn publish",
    "types": "tsc --noEmit false --declaration --declarationDir types",
    "typescript": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Evan-Peuvergne/react-roads.git"
  },
  "keywords": [
    "react",
    "router",
    "routing",
    "transitions"
  ],
  "author": {
    "name": "Evan Peuvergne",
    "email": "evanpeuvergne@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Evan-Peuvergne/react-roads/issues"
  },
  "homepage": "https://github.com/Evan-Peuvergne/react-roads#readme",
  "dependencies": {
    "page": "^1.10.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.8.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/page": "^1.8.0",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.10",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^1.0.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "typescript": "^3.3.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  }
